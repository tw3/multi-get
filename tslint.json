{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "object-literal-sort-keys": false,
    "trailing-commas": false,
    "no-console": false
  },
  "rulesDirectory": []
}